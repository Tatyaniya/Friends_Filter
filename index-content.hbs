<div class="container">
    <div class="logo">
        <div class="logo_img"></div>
    </div>
    <div class="content">
        <header class="header">
            <div class="header_title">Выберите друзей</div>
            <div class="header_icon">

            </div>
        </header>
        <div id="search">
            <div class="searching" id="search_left">
                <input type="text" placeholder="Начните вводить имя друга">
            </div>
            <div class="searching" id="search_right">
                <input type="text" placeholder="Название">
            </div>
        </div>
        <section class="lists">
            <div id="lists-left">
                <div class="text">Ваши друзья</div>
                
            </div>
            <div id="lists-right">
                <div class="text">Друзья в списке</div>
                
            </div>
        </section>
        <footer class="footer">
            <button id="save">Сохранить</button>
        </footer>
        <script id="user-template" type="text/x-handlebars-template">
            <div class="friends">
                {{#each items}}
                <div class="friend">
                    <img src="{{photo_50}}"/>
                    <div>{{first_name}} {{last_name}}</div>
                </div>
                {{/each}}
            </div>
        </script>
    </div>
</div>
