<div class="friends" id="friends">
    {{#each items}}
    <div class="friend">
        <img class="friend_img" src="{{photo_100}}"/>
        <div class="friend_text">{{first_name}} {{last_name}}</div>
        <div class="friend_plus" id="plus">
            <img src="src/icons/plus.png" alt="plus" data-id="{{id}}">
        </div>
    </div>
    {{/each}}
</div>